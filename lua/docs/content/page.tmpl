				{{ if or .Properties .BaseProperties }}
				<h2><a id="properties" href="#properties">Properties</a></h2>

					{{ range $index, $property := .Properties }}
						{{ if not $property.Hide }}
							<a id="property-{{ GetAnchorLink .Name }}"></a>
							<div class="object-element-tbl">
